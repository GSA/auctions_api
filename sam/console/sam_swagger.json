{
	"apiVersion": "1.0.0"
	"swaggerVersion": "1.2",
	"basePath": "http://api.data.gov/sam/v1",
	"resourcePath": "/registrations",
	"produces": ["application/json"],
	"apis": [{
		"path": "/registrations/{registrationId}",
		"description": "Operations about entity registrations",
		"operations": [{
			"method": "GET",
			"nickname": "getData",
			"type": "SamRegistration",
			"parameters": [{
				"paramType": "path",
				"name": "registrationId",
				"description": "DUNS+4 number (no '-' sign) of the registration to be fetched",
				"dataType": "string",
				"required": "true"
			},
			{
				"paramType": "query",
				"name": "api_key",
				"description": "An API Key from api.data.gov",
				"dataType": "string",
				"required": "true"
			}],
			"summary": "Find a registration by its DUN+4 number",
			"notes": "If the entity only has a 9-digit DUNS number, pad the end to include 4 zeros",
			"responseMessages": [{
				"code": "200",
				"message": "Ok",
				"responseModel": "registrationType"
			},
			{
				"code": "403",
				"message": "Request for IGT only registration data is forbidden"
			},
			{
				"code": "403",
				"message": "Request for non-public registration data is forbidden"
			},
			{
				"code": "404",
				"message": "The registration could not be found"
			},
			{
				"code": "406",
				"message": "Duns number must be 13 digit long"
			},
			{
				"code": "500",
				"message": "Internal error"
			}]
		}]
	}],
	"models": {
		"SamRegistration": {
			"id": "SAM Registration",
			"description": "A partial representation of a SAM registration",
			"type": "object",
			"properties": {
				"duns": {
					"type": "string",
					"required": true
				},
				"duns4": {
					"type": "string"
				},
				"cage": {
					"type": "string"
				},
				"ncage": {
					"type": "string"
				},
				"dodaac": {
					"type": "string"
				},
				"purposeOfRegistration": {
					"type": "string",
					"enum": ["IGT_ONLY",
					"IGT_FEDERAL_ASSISTANCE",
					"IGT_ALL_AWARDS",
					"FEDERAL_ASSISTANCE",
					"ALL_AWARDS"],
					"required": true
				},
				"registrationDate": {
					"type": "date",
					"required": true
				},
				"expirationDate": {
					"type": "date",
					"required": true
				},
				"activationDate": {
					"type": "date",
					"required": true
				},
				"lastUpdatedDate": {
					"type": "date",
					"required": true
				},
				"legalBusinessName": {
					"type": "string",
					"required": true
				},
				"doingBusinessAsName": {
					"type": "string"
				},
				"companyDivision": {
					"type": "string"
				},
				"divisionNumber": {
					"type": "string"
				},
				"samAddress": {
					"$ref": "#/definitions/SamAddress"
				},
				"congressionalDistrict": {
					"type": "string"
				},
				"businessStartDate": {
					"type": "date"
				},
				"fiscalYearCloseDay": {
					"type": "string"
				},
				"corporateUrl": {
					"type": "string",
					"required": true
				},
				"corporateStructureCode": {
					"type": "string",
					"enum": ["2J",
					"2K",
					"2L",
					"8H",
					"2A",
					"CY",
					"X6",
					"ZZ"],
					"required": true
				},
				"corporateStructureDescription": {
					"type": "string"
				},
				"mailingAddress": {
					"$ref": "#/definitions/SamAddress",
					"required": "true"
				},
				"stateOfIncorporation": {
					"type": "string"
				},
				"countryOfIncorporation": {
					"type": "string"
				},
				"govtBusinessPoc": {
					"$ref": "#/definitions/SamPointOfContact"
				},
				"altGovtBusinessPoc": {
					"$ref": "#/definitions/SamPointOfContact"
				},
				"pastPerformancePoc": {
					"$ref": "#/definitions/SamPointOfContact"
				},
				"altPastPerformancePoc": {
					"$ref": "#/definitions/SamPointOfContact"
				},
				"electronicBusinessPoc": {
					"$ref": "#/definitions/SamPointOfContact"
				},
				"altElectronicBusinessPoc": {
					"$ref": "#/definitions/SamPointOfContact"
				},
				"creditCardUsage": {
					"type": "boolean"
				},
				"hasDelinquentFederalDebt": {
					"type": "boolean"
				},
				"hasKnownExclusion": {
					"type": "boolean"
				},
				"correspondenceFlag": {
					"type": "string",
					"enum": ["M",
					"F",
					"E"]
				},
				"status": {
					"type": "string",
					"enum": ["DRAFT",
					"WIP_TIN_MATCH_NOT_SUBMITTED",
					"WIP_TIN_MATCH_SUBMITTED",
					"SUBMITTED_TO_TIN_MATCH",
					"SUBMITTED_TO_CAGE_VALIDATION",
					"ACTIVE",
					"EXPIRED",
					"DELETED",
					"STATUS_ERROR"],
					"required": true
				},
				"statusMessage": {
					"type": "string"
				},
				"businessTypes": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"required": true
				},
				"naics": {
					"type": "array",
					"items": {
						
					}
				},
				"sbaCertificationExpirationDate": {
					"type": "date"
				},
				"disasterRelief": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"state": {
								"type": "string"
							},
							"county": {
								"type": "string"
							},
							"msa": {
								"type": "string"
							}
						}
					}
				},
				"pscCodes": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"pscCode": {
								"type": "string",
								"required": true
							},
							"pscName": {
								"type": "string"
							},
							"isPrimary": {
								"type": "boolean"
							},
							"smallBusinessIndicator": {
								"type": "string",
								"enum": ["Y",
								"N",
								"E"]
							}
						}
					}
				},
				"certifications": {
					"type": "object",
					"properties": {
						"pdfUrl": {
							"type": "string",
							"required": true
						}
					}
				},
				"qualifications": {
					"type": "object",
					"properties": {
						"pdfUrl": {
							"type": "string",
							"required": true
						}
					}
				}
			}
		},
		"SamPointOfContact": {
			"description": "A representation of a point of contact in SAM",
			"type": "object",
			"properties": {
				"firstName": {
					"type": "string",
					"required": true
				},
				"lastName": {
					"type": "string",
					"required": true
				},
				"middleInitial": {
					"type": "string"
				},
				"address": {
					"$ref": "#/definitions/SamAddress"
				},
				"usPhone": {
					"type": "string"
				},
				"usPhoneExt": {
					"type": "string"
				},
				"nonUsPhone": {
					"type": "string"
				},
				"nonUsPhoneExt": {
					"type": "string"
				},
				"email": {
					"type": "string",
					"required": true
				}
			}
		},
		"SamAddress": {
			"description": "A representation of an address in SAM",
			"type": "object",
			"properties": {
				"line1": {
					"type": "string",
					"required": true
				},
				"line2": {
					"type": "string"
				},
				"city": {
					"type": "string",
					"required": true
				},
				"stateOrProvince": {
					"type": "string"
				},
				"zip": {
					"type": "string"
				},
				"zip4": {
					"type": "string"
				},
				"countryCode": {
					"type": "string",
					"required": true
				}
			}
		},
		"SamNaics": {
			"description": "A representation of a NAICS code entry",
			"type": "object",
			"properties": {
				"naicsCode": {
					"type": "string",
					"required": true
				},
				"naicsName": {
					"type": "string"
				},
				"isPrimary": {
					"type": "boolean"
				},
				"hasException": {
					"type": "boolean"
				},
				"smallBusinessIndicator": {
					"type": "string",
					"enum": ["Y",
					"N",
					"E"]
				},
				"smallBusinessClassification": {
					"type": "string",
					"enum": ["A4",
					"A6",
					"JT",
					"XX"]
				},
				"hasProtest": {
					"type": "boolean"
				}
			}
		},
		"SamFarResponse": {
			"description": "A representation of responses to FAR or DFAR certifications and qualifications",
			"type": "object",
			"properties": {
				"id": {
					"type": "string",
					"required": true
				},
				"answers": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"answerId": {
								"type": "string",
								"required": true
							},
							"answer": {
								"type": "object",
								"oneOf": [{
									"ref": "#/description/SamNaics"
								},
								{
									"ref": "#/description/SamAddress"
								},
								{
									"ref": "#/description/SamPointOfContact"
								},
								{
									"type": "object",
									"properties": {
										"answerId": {
											"type": "string",
											"required": "true"
										}
									}
								}]
							}
						}
					}
				}
			}
		}
	}
}
